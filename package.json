{
    "name": "Stata",
    "displayName": "Stata Enhanced",
    "description": "Enhanced Stata syntax highlighting in Visual Studio Code",
    "version": "0.5.16",
    "publisher": "kylebarron",
    "engines": {
        "vscode": "^1.0.0"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/kylebarron/language-stata/issues"
    },
    "homepage": "https://github.com/kylebarron/language-stata/tree/vscode",
    "repository": {
        "type": "git",
        "url": "https://github.com/kylebarron/language-stata.git"
    },
    "categories": [
        "Languages"
    ],
    "galleryBanner": {
        "color": "#1d5f6d",
        "theme": "dark"
    },
    "contributes": {
        "languages": [{
            "id": "stata",
            "aliases": ["Stata Enhanced", "Stata", "stata"],
            "extensions": [".do",".ado",".mata"],
            "configuration": "./language_config.json"
        }],
        "grammars": [{
            "language": "stata",
            "scopeName": "source.stata",
            "path": "./grammars/stata.json"
        }]
    }
}